{
  "name": "json-logic-engine",
  "version": "1.1.8",
  "description": "Construct complex rules with JSON & process them.",
  "main": "./dist/cjs.js",
  "module": "./index.js",
  "author": "Jesse Daniel Mitchell",
  "repository": {
    "url": "https://github.com/TotalTechGeek/json-logic-engine"
  },
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules nyc jest --coverage",
    "coverage": "coveralls < coverage/lcov.info",
    "prepublish": "npm run build",
    "build": "rm -rf dist && rollup index.js --file dist/cjs.js --format cjs --exports named && cd dist && standard --fix cjs.js"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.3.1",
    "nyc": "^15.1.0",
    "rollup": "^2.58.3"
  },
  "engines": {
    "node": ">=12.22.7"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./bench"
    ]
  },
  "exports": {
    "require": "./dist/cjs.js",
    "import": "./index.js"
  },
  "type": "module"
}
